    "react-native-reanimated": "^3.17.1",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.14.1",
    "react-native-screens": "~4.4.0",
    "react-native-svg": "^15.11.2",
    "react-native-webview": "^13.13.4",
